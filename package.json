    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-simple-maps": "^3.0.0",
    "react-tinacms": "^0.9.27",
    "react-tinacms-github": "^0.52.0",
    "tinacms": "^2.8.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@tinacms/cli": "^1.10.2",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "daisyui": "^5.0.50",
